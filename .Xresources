Xft*dpi:                    120
Xft*antialias:              true
Xft*autohint:               true
Xft*rgba:                   vbgr

vim*scrollBar:              false
vim.iso14755:               1
vim.iso14755_52:            1

vim.skipBuiltinGlyphs:      1
vim.insecure:               1

yemen.iso14755:             1
yemen.iso14755_52:          1
yemen.skipBuiltinGlyphs:    0
yemen.insecure:             1

yemen.buffered:             0
yemen.internalBorder:       0
yemen.externalBorder:       0

! yemen.font:                 xft:Source Code Pro for Powerline:pixelsize=16:embolden=1
! yemen.font:                 xft:Neep Alt:style=SemiCondensed:pixelsize=11,xft:Inconsolata\\-g for Powerline:pixelsize=14:embolden=1
yemen.font:                 xft:Inconsolata\\-g for Powerline:pixelsize=14:embolden=1
! yemen.boldFont:             xft:Source Code Pro for Powerline:pixelsize=16:embolden=1:style=Bold
! yemen.boldFont:             xft:Neep Alt:style=Bold SemiCondensed:pixelsize=11,xft:Inconsolata\\-g for Powerline:pixelsize=14:embolden=1:style=Bold
yemen.boldFont:             xft:Inconsolata\\-g for Powerline:pixelsize=14:embolden=1:style=Bold
yemen.italicFont:           xft:Profont for Powerline:pixelsize=16:embolden=1
yemen.boldItalicFont:       -misc-fixed-medium-normal--7-70-75-75-c-50-koi8


yemen*cutchars:             "()*,<>[]{}|'"
yemen*matcher.pattern.1:    \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
yemen*matcher.button:       1
yemen*matcher.launcher.1:   /home/polychoron/bin/ff
yemen*underlineURLs:        false
yemen*urlLauncher:          /home/polychoron/bin/ff

yemen.jumpScroll:           true
yemen.loginShell:           true
yemen.saveLines:            4096
yemen.scrollBack:           true
yemen.scrollKey:            true
yemen.scrollTtyKeypress:    true
yemen.scrollTtyOutput:      false
yemen.scrollWithBuffer:     true
yemen.skipScroll:           true
yemen.trippleclickwords:    true
yemen.urgentOnBell:         false

yemen.pasteableTabs:        false
yemen.pointerBlank:         false
yemen.pointerBlankDelay:    2
yemen.scrollstyle:          next
yemen.scrollBar:            false
yemen.depth:                24

URxvt.perl-ext-common:      default,clipboard,selection-to-clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
! URxvt.keysym.M-u:           perl:url-select:select_next
! URxvt.keysym.M-Escape:      perl:keyboard-select:activate
! URxvt.keysym.M-s:           perl:keyboard-select:search
! URxvt.keysym.Shift-Control-C : perl:clipboard:copy
! URxvt.keysym.Shift-Control-V : perl:clipboard:paste

*.foreground:   #c6c6c6
*.background:   #1f1f1f
*.cursorColor:  #c6c6c6

*.color0:       #828282
*.color8:       #828282
*.color1:       #cc6666
*.color9:       #cc6666
*.color2:       #81a2be
*.color10:      #81a2be
*.color3:       #f0c674
*.color11:      #f0c674
*.color4:       #81a2be
*.color12:      #81a2be
*.color5:       #b294bb
*.color13:      #b294bb
*.color6:       #8abeb7
*.color14:      #8abeb7
*.color7:       #c5c8c6
*.color15:      #c5c8c6
!*fading:                15
!*fadeColor:             black
*pointerColorBackground:#2B2C2B
*pointerColorForeground:#16A085

! ROFI SETUP HERE !

! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: #1f1f1f, #c6c6c6, #c6c6c6
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #1f1f1f, #828282, #1f1f1f, #1f1f1f, #c6c6c6
rofi.color-active: #1f1f1f, #828282, #1f1f1f, #1f1f1f, #c6c6c6
rofi.color-urgent: #1f1f1f, #828282, #1f1f1f, #1f1f1f, #c6c6c6

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: monofur 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true

